
!

*foreground: #fbf1c7
*foreground_bold: #a8a19f 
*cursor: #a8a19f
*background: #282828


!black
*color0:      #282828
*color8:      #888888
!red
*color1:      #ff8278
*color9:      #ff8278
!green
*color2:      #bde077
*color10:     #bde077
!yellow
*color3:      #eadc84
*color11:     #eadc84
!blue
*color4:      #77bee0
*color12:     #77bee0
!magenta
*color5:      #dd91f3
*color13:     #dd91f3
!cyan
*color6:      #ffc178
*color14:     #ffc178
!white
*color7:      #dddddd
*color15:     #cccccc



URxvt.intensityStyles:	false
URxvt.background:       [50]#32302f
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.font: 		xft:monospace:size=14
URxvt.depth:		32

!!Transparency
Urxvt.transparent: 	true
URxvt.shading:		100


!! resize-font
URxvt.keysym.C-j:     resize-font:smaller
URxvt.keysym.C-k:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

!!clickable urls
URxvt.perl-ext-common: default,matcher, selection-to-clipboard, resize-font
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k
URxvt.url-launcher: /usr/bin/qutebrowser
URxvt.matcher.button: 1

